    "react": "^17.0.1",
    "react-clipboard.js": "^2.0.16",
    "react-collapse": "^5.1.0",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3",
    "react-hot-loader": "^4.13.0",
    "react-resizable": "^3.0.4",
    "react-scroll-to-bottom": "^4.1.0",
    "sass": "^1.54.3",
    "sass-loader": "^10.1.0",
    "scrollparent": "^2.0.1",
    "semver": "^7.3.4",
    "smoothscroll-polyfill": "^0.4.4",
    "source-map-loader": "^2.0.0",
    "style-loader": "^2.0.0",
