  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.jsx": "eslint",
    "*.ts": "eslint --ext .ts",
    "*.tsx": "eslint --ext .tsx"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
